	"print_r": {
		"prefix": "pr",
		"body": [
			"echo '<pre>';print_r($1); echo '<pre>';",
		],
		"description": "print_r"
	},

	"print_d": {
		"prefix": "pd",
		"body": [
			"echo '<pre>';print_r($1); echo '<pre>';die;",
		],
		"description": "print_r"
	},

	"result": {
		"prefix": "resd",
		"body": [
			"\\$result = array();",
